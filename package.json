{
  "name": "ownmusic",
  "version": "1.0.0",
  "description": "ownmusic is a new platform that is built to support new artists",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint ./src",
    "test": "jest",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "build": "npm run build-fe && npm run build-be",
    "be": "nodemon --exec babel-node ./src/backend/server.js --watch . --ignore 'node_modules'",
    "fe": "webpack serve --mode development --env development",
    "build-fe": "NODE_ENV=production webpack --config webpack-prod.config.js",
    "build-be": "babel ./src/backend -D --out-dir dist",
    "build:css": "postcss src/frontend/assets/tailwind.css -o src/frontend/assets/main.css",
    "watch:css": "postcss -w src/frontend/assets/tailwind.css -o src/frontend/assets/main.css",
    "prebuild": "NODE_ENV=production npm run build:css"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "formik": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pm2": "^4.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "validator": "^13.1.17"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-react": "0.0.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.1",
    "mini-css-extract-plugin": "^1.2.1",
    "nodemon": "^2.0.6",
    "postcss-cli": "^8.2.0",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "react-test-render": "^1.1.2",
    "redux-devtools": "^3.7.0",
    "redux-devtools-extension": "^2.13.8",
    "set-cookie-parser": "^2.4.6",
    "style-loader": "^2.0.0",
    "superagent": "^6.1.0",
    "supertest": "^5.0.0",
    "tailwindcss": "^1.9.6",
    "webpack": "^5.3.2",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-dynamic-import"
    ]
  }
}
